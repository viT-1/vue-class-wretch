{
	"name": "issue-vue-property-decorator-285",
	"version": "0.1.1",
	"keywords": [
		"esm",
		"nobundle",
		"without gulp",
		"without webpack",
		"without rollup",
		"typescript",
		"vue",
		"vue-class-component"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/viT-1/issue-vue-property-decorator-285.git"
	},
	"dependencies": {
		"express": "4.17.1",
		"vue": "2.6.10",
		"vue-class-component": "7.1.0"
	},
	"devDependencies": {
		"@types/express": "4.11.1",
		"esm": "3.2.25",
		"replace-in-file": "4.2.0",
		"shx": "0.3.2",
		"typescript": "^3.7.2"
	},
	"scripts": {
		"predeploy": "shx rm -rf dist",
		"deploy": "tsc",
		"postdeploy": "npm run cindex | npm run ccompesm | npm run resolveesm | npm run www",
		"ccompesm": "shx cp ./node_modules/vue-class-component/dist/vue-class-component.esm.js dist",
		"cindex": "shx cp src/index.html dist",
		"resolveesm": "replace-in-file --configFile=replace.config.js",
		"www": "node -r esm ./dist/express.js"
	}
}
